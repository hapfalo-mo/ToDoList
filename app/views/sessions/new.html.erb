<h1>Login</h1>
<%= form_with url: sessions_path, method: :post do %>
  <div>
    <%= label_tag :email %><br>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div>
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
  </div>
  <div>
    <%= submit_tag "Login" %>
  </div>
<% end %>
